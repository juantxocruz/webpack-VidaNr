    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "install": "^0.13.0",
    "jest": "^27.4.5",
    "mini-css-extract-plugin": "^2.5.1",
    "npm": "^8.3.1",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^7.2.3",
    "rimraf": "^3.0.2",
    "sass": "^1.77.5",
