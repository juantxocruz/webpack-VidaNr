{
  "name": "nacionalRe VidaNr",
  "version": "1.0.0",
  "description": "VidaNr calculator frontend project based on webpack 5.",
  "scripts": {
    "lint": "npm run lint:styles && npm run lint:scripts",
    "lint:styles": "stylelint src",
    "lint:scripts": "eslint src",
    "start": "cross-env SERVE=true NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js --open",
    "build:nacionalRe": "cross-env NODE_ENV=production ASSET_PATH=/calculadora-vidanr/build/ webpack --config webpack/webpack.config.prod.js",
    "build:botsoul": "cross-env NODE_ENV=production ASSET_PATH=/pruebas/nacionalRe-vidaNr/build/ webpack --config webpack/webpack.config.prod.js",
    "clean": "rimraf ./build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juantxocruz/webpack-vidaNr.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Juantxo Cruz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juantxocruz/webpack-vidaNr"
  },
  "devDependencies": {
